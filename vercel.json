{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "framework": null,
  "buildCommand": "npm run build:web",
  "outputDirectory": "web/dist",
  "installCommand": "npm install",
  "functions": {
    "api/src/vercel.ts": {}
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/src/vercel.ts"
    }
  ],
  "crons": [
    {
      "path": "/api/sync/cron",
      "schedule": "*/5 * * * *"
    }
  ]
}
